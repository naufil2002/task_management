{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "scraping/scrape.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/scrape",
        "dest": "/scraping/scrape.py"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }
  