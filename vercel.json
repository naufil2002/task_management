{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/react"
      },
      {
        "src": "scraping/index.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/scrape",
        "dest": "/scraping/index.py"
      },
      {
        "src": "/(.*)",
        "dest": "/frontend/$1"
      }
    ]
  }
  